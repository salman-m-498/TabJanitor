{
  "manifest_version": 3,
  "name": "Tab Janitor",
  "version": "1.0",
  "description": "Automatically archives excess tabs to keep your browser fast.",
  "permissions": ["tabs", "storage", "notifications", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}