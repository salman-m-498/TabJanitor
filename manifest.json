{
  "manifest_version": 3,
  "name": "Tab Janitor",
  "version": "1.0",
  "description": "Automatically archives excess tabs to keep your browser fast.",
  "permissions": ["tabs", "storage", "notifications", "scripting", "alarms"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icons/favicon-128x128.png",
    "48": "icons/favicon-128x128.png",
    "128": "icons/favicon-128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/favicon-128x128.png",
      "48": "icons/favicon-128x128.png",
      "128": "icons/favicon-128x128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}